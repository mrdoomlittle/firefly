cc=$1
cc_flags=$2
syput "cc" cc
syput "cc_flags" cc_flags
shell cas "$cc $cc_flags -c -o dus.o dus.c"
shell cas "$cc $cc_flags -c -o exec.o exec.c"
shell cas "$cc $cc_flags -c -o exp.o exp.c"
shell cas "$cc $cc_flags -c -o lexer.o lexer.c"
shell cas "$cc $cc_flags -c -o parser.o parser.c"
shell cas "$cc $cc_flags -c -o gen.o gen.c"
shell cas "$cc $cc_flags -c -o hash.o hash.c"
